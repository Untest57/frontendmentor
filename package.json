    "@html-eslint/eslint-plugin": "^0.27.0",
    "@html-eslint/parser": "^0.27.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "fast-glob": "^3.3.2",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss-html": "^1.7.0",
    "prettier": "3.3.3",
    "sass-embedded": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-html": "^1.1.0",
